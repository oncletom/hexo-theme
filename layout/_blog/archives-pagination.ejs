<% if (page.prev){ %>
<div class="span5 navigation" id="next-page">
  <div class="article nav-link"><%- link_to(page.prev_link, "&laquo; Page précédente") %></div>
</div>
<% } %>
<% if (page.next){ %>
<div class="span5 navigation" id="previous-page">
  <div class="article nav-link"><%- link_to(page.next_link, "Page suivante &raquo;") %></div>
</div>
<% } %>