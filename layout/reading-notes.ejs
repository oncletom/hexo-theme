<%- partial('header') %>
<%- partial('cover/hello', { post: page }) %>

<main class="timeline" role="main">
  <section>
    <%- partial("blog/post-single", {
      post: page,
      features: ["content"]
    }) %>
  </section>

  <section class="timeline-section">
    <ol class="card-list">
    <% site.pages.find({ layout: 'reading-note' }).sort('updated', -1).forEach(post => { %>
      <li class="card-list__item">
        <%- partial("photography/card", { post, features: ["date"] }) %>
      </li>
    <% }) %>
    </ol>
  </section>
</main>

<%- partial("footer", {}, { cache: true }) %>
