{
  "name": "hexo-theme-oncletom",
  "version": "2.0.0",
  "private": true,
  "description": "Hexo theme for my portfolio and stuff.",
  "main": "index.js",
  "scripts": {
    "build": "run-p 'build:*'",
    "build:js": "rollup --config",
    "build:css": "postcss source/css/*.css --config postcss.config.js --dir source/styles",
    "copy:vendor": "cp -rf node_modules/reveal.js/plugin/notes source/vendor && git add source/vendor/notes",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "run-p 'watch:*'",
    "watch:js": "rollup --config --watch",
    "watch:css": "postcss source/css/*.css --watch --verbose --config postcss.config.js --dir source/styles --map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oncletom/hexo-theme.git"
  },
  "keywords": [
    "hexo"
  ],
  "author": "Thomas Parisot (https://oncletom.io)",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/oncletom/hexo-theme/issues"
  },
  "homepage": "https://github.com/oncletom/hexo-theme#readme",
  "dependencies": {
    "group-array": "^0.3.3",
    "highlight.js": "^9.13.1",
    "normalize.css": "^8.0.0",
    "reading-time": "^1.1.3",
    "reveal-random-colors": "^1.0.1",
    "reveal.js": "^3.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "ejs": "^2.6.1",
    "npm-run-all": "^4.1.3",
    "postcss": "^7.0.5",
    "postcss-cli": "^6.0.1",
    "postcss-extend-rule": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.0.10",
    "postcss-url": "^8.0.0",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-browserify-transform": "^1.0.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-string": "^2.0.2"
  }
}
